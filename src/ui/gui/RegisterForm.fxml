                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="45.0" prefWidth="360.0">
               <children>
                  <Label text="Password">
                     <HBox.margin>
                        <Insets left="20.0" right="70.0" />
                     </HBox.margin>
                  </Label>
                  <PasswordField fx:id="passwordField">
                     <HBox.margin>
                        <Insets left="-57.0" right="50.0" />
                     </HBox.margin>
                  </PasswordField>
               </children>
            </HBox>
            <HBox alignment="CENTER">
               <children>
                  <Label text="Personal details">
                     <HBox.margin>
                        <Insets bottom="10.0" right="35.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="45.0" prefWidth="360.0">
               <children>
                  <Label text="First name">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="firstNameField">
                     <HBox.margin>
                        <Insets right="30.0" />
