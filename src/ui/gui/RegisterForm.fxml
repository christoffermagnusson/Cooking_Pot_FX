            <HBox alignment="CENTER" prefHeight="33.0" prefWidth="360.0">
               <children>
                  <Label text="Register new account" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="45.0" prefWidth="360.0">
               <children>
                  <Label text="Email" />
